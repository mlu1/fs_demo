#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/mlumt/Work/fs_demo/project4/trpc-server-demo/node_modules/.pnpm/@nrwl+tao@19.6.2/node_modules/@nrwl/tao/node_modules:/mnt/c/Users/mlumt/Work/fs_demo/project4/trpc-server-demo/node_modules/.pnpm/@nrwl+tao@19.6.2/node_modules/@nrwl/node_modules:/mnt/c/Users/mlumt/Work/fs_demo/project4/trpc-server-demo/node_modules/.pnpm/@nrwl+tao@19.6.2/node_modules:/mnt/c/Users/mlumt/Work/fs_demo/project4/trpc-server-demo/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/mlumt/Work/fs_demo/project4/trpc-server-demo/node_modules/.pnpm/@nrwl+tao@19.6.2/node_modules/@nrwl/tao/node_modules:/mnt/c/Users/mlumt/Work/fs_demo/project4/trpc-server-demo/node_modules/.pnpm/@nrwl+tao@19.6.2/node_modules/@nrwl/node_modules:/mnt/c/Users/mlumt/Work/fs_demo/project4/trpc-server-demo/node_modules/.pnpm/@nrwl+tao@19.6.2/node_modules:/mnt/c/Users/mlumt/Work/fs_demo/project4/trpc-server-demo/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../index.js" "$@"
else
  exec node  "$basedir/../../index.js" "$@"
fi
